# Environment Configuration Template
# Copy this file to .env.dev, .env.stg, or .env.prod and update values

# Browser Mode
# true = headless (không hiển thị browser)
# false = headed (hiển thị browser)
HEADLESS=false

# Number of parallel workers
# Số test chạy đồng thời
WORKERS=4

# Base URL for testing
BASE_URL=https://demoqa.com

# Optional: Slow motion in milliseconds
# Làm chậm mỗi action để dễ debug
# SLOW_MO=500

# Optional: Username and Password (for production environment)
# USERNAME=your_username
# PASSWORD=your_password

